{
	"component_name": "eva",
	"fields": {
		"eva_time": {
			"type": "float",
			"algorithm": "linear_growth",
			"start_value": 0,
			"growth_rate": 1,
			"max_value": 1350
		},
		"batt_time_left": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 1350,
			"end_value": 0,
			"duration_seconds": 1350
		},
		"oxy_pri_storage": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 100,
			"end_value": 0,
			"duration_seconds": 1350
		},
		"oxy_sec_storage": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 100,
			"end_value": 0,
			"duration_seconds": 1350
		},
		"oxy_pri_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 2800,
			"amplitude": 200,
			"frequency": 0.001
		},
		"oxy_sec_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 2750,
			"amplitude": 150,
			"frequency": 0.0012
		},
		"oxy_time_left": {
			"type": "int",
			"algorithm": "dependent_value",
			"formula": "(oxy_pri_storage + oxy_sec_storage) * 6.75",
			"depends_on": [
				"oxy_pri_storage",
				"oxy_sec_storage"
			]
		},
		"heart_rate": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "95.0 + temperature * 0.6 - 16.0",
			"depends_on": [
				"temperature"
			]
		},
		"oxy_consumption": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "heart_rate * 25.0 - 1500.0",
			"depends_on": [
				"heart_rate"
			]
		},
		"co2_production": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "heart_rate * 24.0 - 1400.0",
			"depends_on": [
				"heart_rate"
			]
		},
		"suit_pressure_oxy": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 4.3,
			"amplitude": 0.2,
			"frequency": 0.02
		},
		"suit_pressure_co2": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "co2_production * 0.0001 + 0.001",
			"depends_on": [
				"co2_production"
			]
		},
		"suit_pressure_other": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 0,
			"end_value": 0,
			"duration_seconds": 900
		},
		"suit_pressure_total": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "suit_pressure_oxy + suit_pressure_co2 + suit_pressure_other",
			"depends_on": [
				"suit_pressure_oxy",
				"suit_pressure_co2",
				"suit_pressure_other"
			]
		},
		"fan_pri_rpm": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "25000.0 + suit_pressure_co2 * 15000.0",
			"depends_on": [
				"suit_pressure_co2"
			]
		},
		"fan_sec_rpm": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 25000,
			"amplitude": 5000,
			"frequency": 0.04
		},
		"helmet_pressure_co2": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "suit_pressure_co2 * 1.2",
			"depends_on": [
				"suit_pressure_co2"
			]
		},
		"scrubber_a_co2_storage": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "helmet_pressure_co2 * 0.8 + 0.1",
			"depends_on": [
				"helmet_pressure_co2"
			]
		},
		"scrubber_b_co2_storage": {
			"type": "float",
			"algorithm": "linear_growth",
			"start_value": 0,
			"growth_rate": 0.08,
			"max_value": 1
		},
		"temperature": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 75,
			"amplitude": 5,
			"frequency": 0.015
		},
		"coolant_ml": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "100.0 - temperature * 1.2 + 54.0",
			"depends_on": [
				"temperature"
			]
		},
		"coolant_gas_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 250,
			"amplitude": 100,
			"frequency": 0.008
		},
		"coolant_liquid_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 450,
			"amplitude": 80,
			"frequency": 0.006
		}
	}
}