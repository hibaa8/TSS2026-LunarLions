{
	"component_name": "eva",
	"fields": {
		"eva_time": {
			"type": "float",
			"algorithm": "linear_growth",
			"start_value": 0,
			"growth_rate": 1,
			"max_value": 1800
		},
		"batt_time_left": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 1800,
			"end_value": 0,
			"duration_seconds": 1800
		},
		"oxy_pri_storage": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 100,
			"end_value": 20,
			"duration_seconds": 1800
		},
		"oxy_sec_storage": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 100,
			"end_value": 20,
			"duration_seconds": 1800
		},
		"oxy_pri_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 2800,
			"amplitude": 200,
			"frequency": 0.001
		},
		"oxy_sec_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 2750,
			"amplitude": 150,
			"frequency": 0.0012
		},
		"oxy_time_left": {
			"type": "int",
			"algorithm": "dependent_value",
			"formula": "(oxy_pri_storage + oxy_sec_storage) * 6.75",
			"depends_on": [
				"oxy_pri_storage",
				"oxy_sec_storage"
			]
		},
		"heart_rate": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "85.0 + (temperature - 21.0) * 2.0",
			"depends_on": [
				"temperature"
			]
		},
		"oxy_consumption": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "heart_rate * 6.5 + 150.0",
			"depends_on": [
				"heart_rate"
			]
		},
		"co2_production": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "heart_rate * 24.0 - 1400.0",
			"depends_on": [
				"heart_rate"
			]
		},
		"suit_pressure_oxy": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 4.1,
			"amplitude": 0.15,
			"frequency": 0.01
		},
		"suit_pressure_co2": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 0.15,
			"amplitude": 0.10,
			"frequency": 0.008
		},
		"suit_pressure_other": {
			"type": "float",
			"algorithm": "linear_decay",
			"start_value": 0,
			"end_value": 0,
			"duration_seconds": 900
		},
		"suit_pressure_total": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "suit_pressure_oxy + suit_pressure_co2 + suit_pressure_other",
			"depends_on": [
				"suit_pressure_oxy",
				"suit_pressure_co2",
				"suit_pressure_other"
			]
		},
		"fan_pri_rpm": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "8000.0 + suit_pressure_co2 * 5000.0",
			"depends_on": [
				"suit_pressure_co2"
			]
		},
		"fan_sec_rpm": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 9000,
			"amplitude": 3000,
			"frequency": 0.04
		},
		"helmet_pressure_co2": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "suit_pressure_co2 * 1.2",
			"depends_on": [
				"suit_pressure_co2"
			]
		},
		"scrubber_a_co2_storage": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "helmet_pressure_co2 * 0.8 + 0.1",
			"depends_on": [
				"helmet_pressure_co2"
			]
		},
		"scrubber_b_co2_storage": {
			"type": "float",
			"algorithm": "linear_growth",
			"start_value": 0,
			"growth_rate": 0.08,
			"max_value": 1
		},
		"temperature": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 21,
			"amplitude": 3,
			"frequency": 0.015
		},
		"coolant_ml": {
			"type": "float",
			"algorithm": "dependent_value",
			"formula": "120.0 - temperature * 2.0",
			"depends_on": [
				"temperature"
			]
		},
		"coolant_gas_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 250,
			"amplitude": 100,
			"frequency": 0.008
		},
		"coolant_liquid_pressure": {
			"type": "float",
			"algorithm": "sine_wave",
			"base_value": 450,
			"amplitude": 80,
			"frequency": 0.006
		}
	}
}