{
  "component_name": "rover",
  "fields": {
    "ac_heating": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.ac_heating"
    },
    "ac_cooling": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.ac_cooling"
    },
    "co2_scrubber": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.co2_scrubber"
    },
    "lights_on": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.lights_on"
    },
    "in_sunlight": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.in_sunlight"
    },
    "throttle": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.throttle"
    },
    "brakes": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.brakes"
    },
    "steering": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.steering"
    },
    "heading": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.heading"
    },
    "current_pos_x": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.current_pos_x"
    },
    "current_pos_y": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.current_pos_y"
    },
    "current_pos_alt": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.current_pos_alt"
    },
    "pitch": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.pitch"
    },
    "roll": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.roll"
    },
    "speed": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.speed"
    },
    "distance_traveled": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.distance_traveled"
    },
    "surface_incline": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 2.0,
      "amplitude": 8.0,
      "frequency": 0.005
    },
    "oxygen_tank": {
      "type": "float",
      "algorithm": "linear_decay",
      "start_value": 100.0,
      "end_value": 20.0,
      "duration_seconds": 675.0
    },
    "oxygen_pressure": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "oxygen_tank * 30.0",
      "depends_on": ["oxygen_tank"]
    },
    "ac_fan_pri": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 15000.0,
      "amplitude": 10000.0,
      "frequency": 0.003
    },
    "ac_fan_sec": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 12000.0,
      "amplitude": 8000.0,
      "frequency": 0.0025
    },
    "cabin_pressure": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 4.0,
      "amplitude": 0.3,
      "frequency": 0.008
    },
    "cabin_temperature": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "external_temp * 0.15 + in_sunlight * 8.0 + ac_heating * 15.0 - ac_cooling * 10.0 + 20.0",
      "depends_on": ["external_temp", "in_sunlight", "ac_heating", "ac_cooling"]
    },
    "battery_level": {
      "type": "float",
      "algorithm": "linear_decay",
      "start_value": 100.0,
      "end_value": 5.0,
      "duration_seconds": 1350.0
    },
    "power_consumption_rate": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "100.0 + speed * 2.0 + lights_on * 75.0 + ac_heating * 500.0 + ac_cooling * 300.0",
      "depends_on": ["speed", "lights_on", "ac_heating", "ac_cooling"]
    },
    "solar_panel_efficiency": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "in_sunlight * 85.0 + 15.0",
      "depends_on": ["in_sunlight"]
    },
    "solar_generation": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "solar_panel_efficiency * 3.5",
      "depends_on": ["solar_panel_efficiency"]
    },
    "external_temp": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": -30.0,
      "amplitude": 50.0,
      "frequency": 0.0008
    },
    "pr_coolant_tank": {
      "type": "float",
      "algorithm": "linear_decay",
      "start_value": 100.0,
      "end_value": 10.0,
      "duration_seconds": 21600.0
    },
    "radiator": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 75.0,
      "amplitude": 25.0,
      "frequency": 0.004
    },
    "motor_temp": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "speed * 5.0 + external_temp * 0.3 + 30.0",
      "depends_on": ["speed", "external_temp"]
    },
    "switch_dest": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.switch_dest"
    },
    "fan_pri": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.fan_pri"
    },
    "dust_wiper": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.dust_wiper"
    },
    "terrain_condition": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "50.0 - surface_incline * 3.0",
      "depends_on": ["surface_incline"]
    },
    "rover_sim_elapsed_time": {
      "type": "float",
      "algorithm": "linear_growth",
      "start_value": 0,
      "end_value": 1350,
      "duration_seconds": 1350.0
    }
  }
}