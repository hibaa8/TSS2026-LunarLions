{
  "component_name": "rover",
  "fields": {
    "cabin_heating": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.cabin_heating",
      "reset_value": false
    },
    "cabin_cooling": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.cabin_cooling",
      "reset_value": false
    },
    "co2_scrubber": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.co2_scrubber",
      "reset_value": false
    },
    "lights_on": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.lights_on",
      "reset_value": false
    },
    "brakes": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.brakes",
      "reset_value": false
    },
    "throttle": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.throttle",
      "reset_value": 0
    },
    "steering": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.steering",
      "reset_value": 0
    },
    "rover_pos_x": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.rover_pos_x",
      "reset_value": 0
    },
    "rover_pos_y": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.rover_pos_y",
      "reset_value": 0
    },
    "rover_pos_z": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.rover_pos_z",
      "reset_value": 0
    },
    "heading": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.heading",
      "reset_value": 0
    },
    "pitch": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.pitch",
      "reset_value": 0
    },
    "roll": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.roll",
      "reset_value": 0
    },
    "distance_traveled": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.distance_traveled",
      "reset_value": 0
    },
    "speed": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.speed",
      "reset_value": 0
    },
    "sunlight": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.sunlight",
      "reset_value": 0
    },
    "surface_incline": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.surface_incline",
      "reset_value": 0
    },
    "oxygen_tank": {
      "type": "float",
      "algorithm": "linear_decay",
      "start_value": 100.0,
      "end_value": 20.0,
      "duration_seconds": 2000.0
    },
    "oxygen_pressure": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "oxygen_tank * 30.0",
      "depends_on": ["oxygen_tank"]
    },
    "fan_pri_rpm": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 30000.0,
      "amplitude": 3.0,
      "frequency": 0.003
    },
    "fan_sec_rpm": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 30000.0,
      "amplitude": 3.0,
      "frequency": 0.0025
    },
    "cabin_pressure": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 4.0,
      "amplitude": 0.1,
      "frequency": 0.008
    },
    "cabin_temperature": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "external_temp * 0.15 + cabin_heating * 15.0 - cabin_cooling * 10.0 + 20.0",
      "depends_on": ["external_temp", "cabin_heating", "cabin_cooling"]
    },
    "battery_level": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "100.0 - rover_elapsed_time * ( 0.02857 + speed * 0.00508 )",
      "depends_on": ["rover_elapsed_time", "speed"]
    },
    "external_temp": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "-173.0 + sunlight * 3.0",
      "depends_on": ["sunlight"]
    },
    "coolant_storage": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "100.0",
      "depends_on": []
    },
    "coolant_pressure": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 500.0,
      "amplitude": 1.0,
      "frequency": 0.01
    },
    "rover_elapsed_time": {
      "type": "float",
      "algorithm": "linear_growth",
      "start_value": 0,
      "max_value": 100000.0
    },
    "signal_strength": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "LTV.json",
      "field_path": "signal.strength",
      "reset_value": 0
    },
    "pings_left": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "LTV.json",
      "field_path": "signal.pings_left",
      "reset_value": 10
    },
    "ping_requested": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "LTV.json",
      "field_path": "signal.ping_requested",
      "reset_value": false
    },
    "dust_connected": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.dust_connected",
      "reset_value": false
    }
  }
}
