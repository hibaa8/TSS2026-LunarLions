    },
    "pr_coolant_pressure": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 500.0,
      "amplitude": 2.0,
      "frequency": 0.01
    },
    "fan_pri": {
      "type": "float",
      "algorithm": "external_value",
      "file_path": "ROVER.json",
      "field_path": "pr_telemetry.fan_pri"
    },
    "rover_sim_elapsed_time": {
      "type": "float",
      "algorithm": "linear_growth",
      "start_value": 0,
