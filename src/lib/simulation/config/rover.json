    "oxygen_tank": {
      "type": "float",
      "algorithm": "linear_decay",
      "start_value": 100.0,
      "end_value": 20.0,
      "duration_seconds": 675.0
    },
    "oxygen_pressure": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "oxygen_tank * 30.0",
      "depends_on": ["oxygen_tank"]
    },
    "ac_fan_pri": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 30000.0,
      "amplitude": 3.0,
      "frequency": 0.003
    },
    "ac_fan_sec": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 30000.0,
      "amplitude": 3.0,
      "frequency": 0.0025
    },
    "cabin_pressure": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": 4.0,
      "amplitude": 0.1,
      "frequency": 0.008
    },
    "cabin_temperature": {
      "type": "float",
      "algorithm": "dependent_value",
      "formula": "external_temp * 0.15 + light_indicator * 8.0 + ac_heating * 15.0 - ac_cooling * 10.0 + 20.0",
      "depends_on": ["external_temp", "light_indicator", "ac_heating", "ac_cooling"]
    },
    "battery_level": {
      "type": "float",
      "algorithm": "linear_decay",
      "start_value": 100.0,
      "end_value": 5.0,
      "duration_seconds": 1350.0
    },
    "external_temp": {
      "type": "float",
      "algorithm": "sine_wave",
      "base_value": -30.0,
      "amplitude": 10.0,
      "frequency": 0.0008
